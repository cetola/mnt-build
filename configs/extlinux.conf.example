	initrd /initramfs-linux-testing
	fdt /imx8mp-mnt-pocket-reform.dtb
	
	append   ro root=/dev/nvme0n1p1 no_console_suspend cryptomgr.notests ${bootargs} console=tty1 loglevel=3

